    <section id="about" class="about">
      <div class="container">

        <div class="section-title">
          <h2>Regional Profiles</h2>
        </div>



        <div class="table-responsive">
        <table class="table table-striped">
          <thead>
            <tr>
              <th scope="col">Name</th>
              <th scope="col">Population</th>
              <th scope="col">County</th>
              <th scope="col">Local govt</th>
              <th scope="col">Description</th>
              <th scope="col">Environment</th>
              <th scope="col">Updated</th>
              <th colspan="3"></th>
            </tr>
          </thead>
          
          
          
          <tbody>
            <% @regional_profiles.each do |regional_profile| %>
            <tr>
              <td><%= regional_profile.name %></td>
              <td><%= regional_profile.population %></td>
              <td><%= regional_profile.county %></td>
              <td><%= regional_profile.local_govt %></td>
              <td><%= regional_profile.description %></td>
              <td><%= regional_profile.environment_id %></td>
              <td><%= regional_profile.updated %></td>
              <% if current_user.try(:admin?) %>
              <td><%= link_to 'Edit', edit_regional_profile_path(regional_profile) %></td>
              <td><%= link_to 'Destroy', regional_profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
              <% end %>
            </tr>
           <% end %> 
          </tbody>
        </table>
        </div>

      </div>
    </section><!-- End About Us Section -->
    
    
    
    <% if current_user.try(:admin?) %>
        <%= link_to 'New Regional Profile', new_regional_profile_path %>
